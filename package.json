{
  "name": "exoptus",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "workspaces": [
    "apps/web-dashboard",
    "server"
  ],
  "scripts": {
    "dev": "concurrently --kill-others \"npm run dev:mobile\" \"npm run dev:admin\" \"npm run dev:server\"",
    "dev:mobile": "expo start",
    "dev:admin": "npm run dev -w exoptus-admin-dashboard",
    "dev:server": "npm run dev -w exoptus-server",
    "build": "npm run build --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present && expo lint",
    "lint:mobile": "expo lint",
    "test": "npm run test --workspaces --if-present",
    "typecheck": "npm run typecheck --workspaces --if-present && npx tsc --noEmit",
    "typecheck:mobile": "npx tsc --noEmit",
    "db:migrate": "npm run db:migrate -w exoptus-server",
    "db:studio": "npm run db:studio -w exoptus-server",
    "db:push": "npm run db:push -w exoptus-server",
    "db:generate": "npm run db:generate -w exoptus-server",
    "clean": "npm run clean --workspaces --if-present && rm -rf node_modules",
    "android:dev": "concurrently --kill-others \"npm:server\" \"npm:mobile-android\"",
    "server": "npm run dev -w exoptus-server",
    "mobile-android": "expo start --android",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/react": "~19.2.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~10.0.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-google-signin/google-signin": "^16.1.1",
    "expo": "~54.0.0",
    "expo-av": "^16.0.8",
    "expo-blur": "~15.0.8",
    "expo-constants": "~18.0.12",
    "expo-document-picker": "~14.0.8",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "lottie-react-native": "~7.3.1",
    "react": "19.2.3",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-worklets": "^0.5.1",
    "react-native-worklets-core": "^1.6.2",
    "zustand": "^5.0.9"
  }
}
