{
  "cli": {
    "version": ">= 3.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://exoptus-server-production.up.railway.app",
        "EXPO_PUBLIC_APP_MODE": "production"
      }
    },
    "production": {
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://exoptus-server-production.up.railway.app",
        "EXPO_PUBLIC_APP_MODE": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
